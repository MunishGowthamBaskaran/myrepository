<script>
	var entity = getUrlParams()["brand"];
	var subProduct = getUrlParams()["subProduct"];
	var selectedState = "tx";
	var stateScoreMap;
	var selectedStore = "all";

	function drawBarChartForCA(words, posCount, negCount, avisPos, avisNeg) {

		jQuery('#container').highcharts(
				{
					chart : {
						type : 'bar'
					},

					tooltip : {
						formatter : function() {
							var stackName = this.series.stackKey.substring(3,
									this.series.stackKey.length);
							var s = '<b>' + this.x + '(' + stackName + ')'
									+ '</b>';

							s += '<br/>' + this.point.series.name + ': '
									+ this.point.y + ' reviews' + '('
									+ Math.round(this.point.percentage) + '%)';
							s += '<br/>' + 'Total' + ': '
									+ this.point.stackTotal + ' Reviews';

							return s;
						},
						shared : false
					},
					title : {
						text : 'Topic wise review score'
					},
					xAxis : {
						categories : words
					},
					yAxis : {
						min : 0,
						title : {
							text : 'Percentage score'
						}
					},
					legend : {
						reversed : true
					},
					plotOptions : {
						series : {
							dataLabels : {
								enabled : true,
								color : 'black',

								formatter : function() {
									var s = Math.round(this.point.percentage)
											+ '%';
									return s;

								}
							},
							stacking : "percent"

						}

					},
					series : [ {
						name : 'Positive',
						data : posCount,
						stack : 'Enterprise'
					}, {
						name : 'Negative',
						data : negCount,
						stack : 'Enterprise'
					}, {
						name : 'Avis - Positive',
						data : avisPos,
						stack : 'Avis'
					}, {
						name : 'Avis - Negative',
						data : avisNeg,
						stack : 'Avis'
					} ]
				});
	}

	function drawBarChart(words, posCount, negCount) {

		jQuery('#container').highcharts(
				{
					chart : {
						type : 'bar'
					},

					tooltip : {
						formatter : function() {
							var stackName = this.series.stackKey.substring(3,
									this.series.stackKey.length);
							var s = '<b>' + this.x + '(' + stackName + ')'
									+ '</b>';

							s += '<br/>' + this.point.series.name + ': '
									+ this.point.y + ' reviews' + '('
									+ Math.round(this.point.percentage) + '%)';
							s += '<br/>' + 'Total' + ': '
									+ this.point.stackTotal + ' Reviews';

							return s;
						},
						shared : false
					},
					title : {
						text : 'Topic wise review score'
					},
					xAxis : {
						categories : words
					},
					yAxis : {
						min : 0,
						title : {
							text : 'Percentage score'
						}
					},
					legend : {
						reversed : true
					},
					plotOptions : {
						series : {
							dataLabels : {
								enabled : true,
								color : 'black',

								formatter : function() {
									var s = Math.round(this.point.percentage)
											+ '%';
									return s;

								}
							},
							stacking : "percent"

						}

					},
					series : [ {
						name : 'Positive',
						data : posCount,
						stack : 'Enterprise'
					}, {
						name : 'Negative',
						data : negCount,
						stack : 'Enterprise'
					} ]
				});
	}
	function getTopTraits(code) {
		jQuery
				.ajax({
					type : "POST",
					url : "/Inferlytics/MapViewConnector",
					data : "type=topSubdimensions&subProduct=" + subProduct
							+ "&entity=" + entity + "&stateCode=" + code,
					dataType : "json",
					async : true,
					success : function(returnData) {
						var htmlToAppend = "<div class='outerContainer'><div>Positive Traits</div></div>";
						var words = [];
						var posCount = [];
						var negCount = [];
						if (returnData.Dimensions.length == 0) {
							jQuery("#container").empty();
							jQuery("#container").text("Data not Found");
						} else {
							jQuery.each(returnData.Dimensions, function(index,
									items) {
								words.push(items.keyWord);
								posCount.push(items.posCount);
								var negCountValue = items.count
										- items.posCount;
								if (negCountValue < 0) {
									negCountValue = -negCountValue;
								}
								negCount.push(negCountValue);

							});
							if (code == "ca") {
								var avisPosCount = [];
								var avisNegCount = [];

								avisPosCount.push(110, 137, 408, 51, 83, 236,
										67, 45, 49, 550);
								avisNegCount.push(29, 65, 163, 21, 26, 190, 38,
										16, 29, 339);
								drawBarChartForCA(words, posCount, negCount,
										avisPosCount, avisNegCount);
							} else {
								drawBarChart(words, posCount, negCount);

							}

						}
					}
				});

	}

	function updateOnMapClick(code) {
		if (code == "ca") {
			jQuery(".scoreDiv").css("width","460px");
			jQuery("#competitorScores").css("display", "inline-block")
		} else {
			jQuery(".scoreDiv").css("width","350px");
			jQuery("#competitorScores").hide();
		}
		getTopTraits(code);
		document.getElementById(code.toLowerCase()).selected = true;
		jQuery(".overAllScore #score").text("0");
		jQuery("#storeCount .count").text("0");

		//Search the json for the avg Score and update it in Ui
		for ( var i in stateScoreMap) {
			if (stateScoreMap[i].id === code) {
				if (stateScoreMap[i].avgScore > 0) {
					jQuery(".overAllScore #score").text(
							"+" + stateScoreMap[i].avgScore.toFixed(2));
					jQuery("#overallScore2").text(
							"+" + stateScoreMap[i].avgScore.toFixed(2));

				} else {
					jQuery(".overAllScore #score").text(
							stateScoreMap[i].avgScore.toFixed(2));
					jQuery("#overallScore2").text(
							stateScoreMap[i].avgScore.toFixed(2));

				}
				jQuery("#storeCount .count").text(stateScoreMap[i].storesCount);

				//loop through the store names and add it as a dropdown list.
				var htmlStoreNames = "<option id='all' value='all'>ALL</option>";
				for ( var j in stateScoreMap[i].storenames) {
					var nameProductId = stateScoreMap[i].storenames[j]
							.split("|");
					htmlStoreNames += "<option id='"+nameProductId[1]+"' value='"+nameProductId[1]+"'>"
							+ nameProductId[0] + "</option>";

				}
				selectedStore = "all";
				jQuery("#storeoptions").html(htmlStoreNames);
			}
		}
		jQuery("#selectedState .count").text(code);
		//Get the Pos and Negative Comments Count for a selected state and display on dashboard.
		jQuery.ajax({
			type : "POST",
			url : "/Inferlytics/MapViewConnector",
			data : "type=commentsCount&subProduct=" + subProduct + "&entity="
					+ entity + "&stateCode=" + code,
			dataType : "json",
			async : true,
			success : function(countData) {
				jQuery("#posCommentCount").text(
						"+" + countData.posCommentsCount);
				jQuery("#negCommentCount").text(
						"-" + countData.negCommentsCount);
			}
		});

	}

	jQuery(document)
			.ready(
					function() {

						setDatesFordatePicker();
						getChartDataForTimeLine();
						jQuery
								.ajax({
									type : "POST",
									url : "/Inferlytics/MapViewConnector",
									data : "type=avgScore&subProduct="
											+ subProduct + "&entity=" + entity,
									dataType : "json",
									async : true,
									success : function(returnData) {
										stateScoreMap = returnData;
										var maxScore = 0;
										var minScore = Number.MAX_VALUE;
										var cc, hex, startColor = [ 0, 100, 0 ], endColor = [
												0, 255, 0 ];
										var colors = {};
										var html = "";

										for (cc in returnData) {
											if (parseFloat(returnData[cc].avgScore) > maxScore) {
												maxScore = parseFloat(returnData[cc].avgScore);
											}
											if (parseFloat(returnData[cc].avgScore) < minScore) {
												minScore = parseFloat(returnData[cc].avgScore);
											}
										}

										for (cc in returnData) {
											html += "<option id='"+returnData[cc].id+"' value='"+returnData[cc].id+"'>"
													+ returnData[cc].id
															.toUpperCase()
													+ "</option>";
											if (returnData[cc].avgScore > 0) {
												colors[returnData[cc].id] = '#';
												for ( var i = 0; i < 3; i++) {
													hex = Math
															.round(
																	endColor[i]
																			+ (startColor[i] - endColor[i])
																			* (returnData[cc].avgScore / (maxScore - minScore)))
															.toString(16);
													if (hex.length == 1) {
														hex = '0' + hex;
													}
													colors[returnData[cc].id] += (hex.length == 1 ? '0'
															: '')
															+ hex;
												}
											}
										}
										jQuery("#statesoptions").html(html);
										jQuery('#vmap')
												.vectorMap(
														{
															map : 'usa_en',
															showTooltip : true,
															selectedRegion : 'TX',
															selectedColor : '#BEBEBE',
															hoverColor : '#c9dfaf',
															colors : colors,
															backgroundColor : null,
															enableZoom : false,
															onRegionClick : function(
																	element,
																	code,
																	region) {

																selectedState = code
																		.toLowerCase();
																updateOnMapClick(code);
															},
															onLabelShow : function(
																	event,
																	label, code) {
																	var score="";		
																	var storeCount="";
																for ( var i in stateScoreMap) {
																	if (stateScoreMap[i].id === code) {
																		if (stateScoreMap[i].avgScore > 0) {
																				score="+"+stateScoreMap[i].avgScore
																				.toFixed(2);
																		} else {
																			score="-"+stateScoreMap[i].avgScore
																			.toFixed(2);
																		}
																			storeCount=stateScoreMap[i].storesCount;
																	}

																}
																label
																		.html("<div ><table><tr><td>State :</td><td>"
																				+ label
																						.text()
																				+ "</td></tr><tr><td>Score :</td><td>"+score+"</td></tr><tr><td>Stores :</td><td>"+storeCount+"</td></tr></table></div>");

															}

														});

										updateOnMapClick("ca");
									}

								});

					});
</script>

